<!--components/w-swiper/w-swiper.wxml-->
<swiper class="swiper" circular autoplay interval="3000" duration="300" indicator-dots="true" indicator-active-color="#FF5777">
  <block wx:for="{{list}}" wx:key="{{index}}">
    <swiper-item class="swipter-item">
      <image src="{{item.image}}"/>
    </swiper-item>
  </block>
</swiper>
